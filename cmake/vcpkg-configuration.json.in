{
  "@KIND_FILE@default-registry": {
    "kind": "filesystem",
    "path": "@DPKG_PORTS_PATH@"
  },
  "@KIND_GIT@default-registry": {
	"kind": "git",
	"baseline": "@DPKG_PORTS_HEAD@",
	"repository": "@DPKG_PORTS_PATH@"
  },
  "registries": [
    {
      "kind": "git",
      "repository": "@VCPKG_ROOT@",
      "baseline": "@VCPKG_HEAD@",
      "packages": [ "vcpkg-cmake", "vcpkg-cmake-config", "libiconv" ]
    }
  ]
}
