{
  "name": "base-files",
  "version": "11.1",
  "maintainers": "Kai Pastor <dg0yt@darc.de>",
  "description": "Debian base system miscellaneous files",
  "homepage": "https://tracker.debian.org/pkg/base-files",
  "supports": "!windows",
  "dependencies": [
    {
      "name": "dpkg-source-util",
      "host": true
    }
  ],
  "features": {
    "all": {
      "description": "All standard license terms",
      "dependencies": [
        {
          "name": "base-files",
          "features": [
            "apache-2-0",
            "artistic",
            "bsd",
            "cc0-1-0",
            "gfdl-1-2",
            "gfdl-1-3",
            "gpl-1",
            "gpl-2",
            "gpl-3",
            "lgpl-2",
            "lgpl-2-1",
            "lgpl-3",
            "mpl-1-1",
            "mpl-2-0"
          ]
        }
      ]
    },
    "apache-20": {
      "description": "Apache-2.0 license terms"
    },
    "artistic": {
      "description": "Artistic license terms"
    },
    "bsd": {
      "description": "BSD license terms"
    },
    "cc0-1-0": {
      "description": "CC0-1.0 license terms"
    },
    "gfdl-1-2": {
      "description": "GFDL-1.2 license terms"
    },
    "gfdl-1-3": {
      "description": "GFDL-1.3 license terms"
    },
    "gpl-1": {
      "description": "GPL-1 license terms"
    },
    "gpl-2": {
      "description": "GPL-2 license terms"
    },
    "gpl-3": {
      "description": "GPL-3 license terms"
    },
    "lgpl-2": {
      "description": "LGPL-2 license terms"
    },
    "lgpl-2-1": {
      "description": "LGPL-2.1 license terms"
    },
    "lgpl-3": {
      "description": "LGPL-3 license terms"
    },
    "mpl-1-1": {
      "description": "MPL-1.1 license terms"
    },
    "mpl-2-0": {
      "description": "MPL-2.0 license terms"
    }
  }
}
